{
  "name": "vue-live-preview",
  "description": "Live Preview Vue.js Plugin",
  "version": "0.0.0",
  "author": "Julien Simonet <j.mons54@gmail.com>",
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "preview",
    "live preview"
  ],
  "license": "MIT",
  "main": "dist/vue-live-preview.common.js",
  "unpkg": "dist/vue-live-preview.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mons54/vue-live-preview.git"
  },
  "bugs": {
    "url": "https://github.com/mons54/vue-live-preview/issues"
  },
  "homepage": "https://github.com/mons54/vue-live-preview#readme",
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-standalone": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.6.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "vue-codemirror": "^4.0.4",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {}
}
